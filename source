SGUI.Parent = game.CoreGui

coroutine.wrap(function()
	local script = Instance.new("LocalScript", HistoryLabel)
	local textLabel = script.Parent
	
	textLabel.Changed:Connect(function()
		task.wait(3)
		textLabel.Text = ""
	end)
end)()

coroutine.wrap(function()
	local script = Instance.new("LocalScript", SGUI)
	local screenGui = script.Parent
	
	for _, button in pairs(screenGui:GetDescendants()) do
		if button:IsA("TextButton") then
			button.MouseButton1Click:Connect(function()
				local historyLabel = screenGui:FindFirstChild("HistoryLabel")
				--===========================================================--
				if button.Name == "Generator" then
					historyLabel.Text = "Enabled Generator ESP"
				elseif button.Name == "Page" then
					historyLabel.Text = "Enabled Page ESP"
				elseif button.Name == "Slender" then
					historyLabel.Text = "Enabled Slender ESP"
				elseif button.Name == "FullBright" then
					historyLabel.Text = "Enabled Fullbright"
				elseif button.Name == "PoopEggs" then
					historyLabel.Text = "Toggled Place Eggs"
				elseif button.Name == "UndetectedSpeedHack" then
					historyLabel.Text = "Clicked Speed Hack"
				elseif button.Name == "Provoke" then
					historyLabel.Text = "Animated Provoke"
				elseif button.Name == "CheatButton" then
					historyLabel.Text = "Toggled UI"
				elseif button.Name == "Refresh" then
					historyLabel.Text = "Clicked Refresh, Refreshing..."
				elseif button.Name == "DestroyGUI" then
					historyLabel.Text = "See you soon"
				end
				--===========================================================--
			end)
		end
	end
	
end)()

